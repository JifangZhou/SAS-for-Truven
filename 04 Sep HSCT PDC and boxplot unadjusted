DATA id_list5;
SET workingD.allo_dm2_masterfile;
RUN;
DATA file_t0;
	SET id_list5;
	KEEP enrolid exposed lipid_baseline_pdc pdc type;
	pdc=lipid_baseline_pdc;
	IF exposed=0 THEN type="unexposed-t0";
	IF exposed=1 THEN type="exposed-t0";
	DROP lipid_baseline_pdc;
RUN;
DATA file_t1;
	SET id_list5;
	KEEP enrolid exposed pdc_t1 pdc type;
	RENAME pdc_t1=pdc;
	IF exposed=0 THEN type="unexposed-t1";
	IF exposed=1 THEN type="exposed-t1";
RUN;
DATA file_t2;
	SET id_list5;
	KEEP enrolid exposed pdc_t2 pdc type;
	RENAME pdc_t2=pdc;
	IF exposed=0 THEN type="unexposed-t2";
	IF exposed=1 THEN type="exposed-t2";
RUN;
DATA file_t3;
	SET id_list5;
	KEEP enrolid exposed pdc_t3 pdc type;
	RENAME pdc_t3=pdc;
	IF exposed=0 THEN type="unexposed-t3";
	IF exposed=1 THEN type="exposed-t3";
RUN;
DATA file_t4;
	SET id_list5;
	KEEP enrolid exposed pdc_t4 pdc type;
	RENAME pdc_t4=pdc;
	IF exposed=0 THEN type="unexposed-t4";
	IF exposed=1 THEN type="exposed-t4";
RUN;
DATA file_t5;
	SET id_list5;
	KEEP enrolid exposed pdc_t5 pdc type;
	RENAME pdc_t5=pdc;
	IF exposed=0 THEN type="unexposed-t5";
	IF exposed=1 THEN type="exposed-t5";
RUN;
DATA file_t6;
	SET id_list5;
	KEEP enrolid exposed pdc_t6 pdc type;
	RENAME pdc_t6=pdc;
	IF exposed=0 THEN type="unexposed-t6";
	IF exposed=1 THEN type="exposed-t6";
RUN;
DATA analytical_file;
	SET file_t0 file_t1 file_t2 file_t3 file_t4 file_t5 file_t6;
	if type="unexposed-t0"  THEN type1=1;
	if type="exposed-t0"  THEN type1=2;
		if type="unexposed-t1"  THEN type1=3;
	if type="exposed-t1"  THEN type1=4;
		if type="unexposed-t2"  THEN type1=5;
	if type="exposed-t2"  THEN type1=6;
		if type="unexposed-t3"  THEN type1=7;
	if type="exposed-t3"  THEN type1=8;
		if type="unexposed-t4"  THEN type1=9;
	if type="exposed-t4"  THEN type1=10;
		if type="unexposed-t5"  THEN type1=11;
	if type="exposed-t5"  THEN type1=12;
		if type="unexposed-t6"  THEN type1=13;
	if type="exposed-t6"  THEN type1=14;
RUN;

PROC SORT data=analytical_file;
	BY type1;
	RUN;
proc boxplot data=analytical_file;
   plot pdc*type/
       nohlabel 
       symbollegend = legend1;
run;
